              version="2.0">

    <lifecycle>
        <phase-listener>org.jboss.webbeans.jsf.WebBeansPhaseListener</phase-listener>
    </lifecycle>

    <factory>
        <application-factory>org.jboss.webbeans.jsf.WebBeansApplicationFactory</application-factory>
    </factory>

    
</faces-config>
