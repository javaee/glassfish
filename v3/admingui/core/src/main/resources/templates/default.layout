<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html xmlns="http://www.w3.org/1999/xhtml" xmlns:ui="http://java.sun.com/jsf/facelets" xmlns:h="http://java.sun.com/jsf/html" xmlns:f="http://java.sun.com/jsf/core">

    <ui:event type="initPage">
        <!--
            I am leaving this in for now in case we decide to
            experiment with alternate layouts later...  Ken
        -->
        if ("!#{sessionScope.guiNavScheme}") {
            setSessionAttribute(key="guiNavScheme" value="tree");
        }
        if ("!#{pageSession.bare}") {
            setPageSessionAttribute(key="bare", value="#{param.bare == 'true'}");
        }
    </ui:event>

    <ui:composition template="/templates/#{pageSession.bare == 'true' ? 'bareLayout.xhtml' : 'treeLayout.xhtml'}" />

</html>
