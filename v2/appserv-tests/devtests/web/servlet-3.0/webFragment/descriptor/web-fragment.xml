<?xml version="1.0" encoding="UTF-8"?>
<web-fragment>
  <display-name>wftest</display-name>
  <distributable></distributable>
  <servlet>
    <servlet-name>wftestServlet</servlet-name>
    <servlet-class>wftest.WFTestServlet</servlet-class>
    <init-param>
      <param-name>mesg</param-name>
      <param-value>hello f</param-value>
    </init-param>
    <init-param>
      <param-name>mesg2</param-name>
      <param-value>hello2 f</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>wftestServlet</servlet-name>
    <url-pattern>/wftest</url-pattern>
  </servlet-mapping>
  <env-entry>
    <env-entry-name>wfmin</env-entry-name>
    <env-entry-type>java.lang.Integer</env-entry-type>
    <env-entry-value>2</env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>wfmax</env-entry-name>
    <env-entry-type>java.lang.Integer</env-entry-type>
    <env-entry-value>10</env-entry-value>
  </env-entry>
</web-fragment>
