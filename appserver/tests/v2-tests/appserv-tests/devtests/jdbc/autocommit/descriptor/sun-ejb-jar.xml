<sun-ejb-jar>
  <enterprise-beans>
    <ejb>
      <ejb-name>SimpleBMPEJB</ejb-name>
      <jndi-name>com.sun.s1asdev.jdbc.autocommit.ejb.SimpleBMPHome</jndi-name>
