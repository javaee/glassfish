            <cache-idle-timeout-in-seconds>600</cache-idle-timeout-in-seconds>
            <removal-timeout-in-seconds>6000</removal-timeout-in-seconds>
            <victim-selection-policy>FIFO</victim-selection-policy>
        </bean-cache>
