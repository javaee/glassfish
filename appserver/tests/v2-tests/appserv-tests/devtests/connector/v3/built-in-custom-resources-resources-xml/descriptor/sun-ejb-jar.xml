<!DOCTYPE sun-ejb-jar PUBLIC '-//Sun Microsystems, Inc.//DTD Sun ONE Application Server 7.0 EJB 2.0//EN' 'http://www.sun.com/software/sunone/appserver/dtds/sun-ejb-jar_2_0-0.dtd'>

<sun-ejb-jar>
  <enterprise-beans>
    <ejb>
      <ejb-name>SimpleBMPEJB</ejb-name>
      <jndi-name>com.sun.s1asdev.jdbc.CustomResourceFactories.ejb.SimpleBMPHome</jndi-name>
      <resource-ref>
        <res-ref-name>custom/my-java-bean</res-ref-name>
        <jndi-name>java:app/custom/my-java-bean</jndi-name>
      </resource-ref>
        <resource-ref>
          <res-ref-name>custom/my-int</res-ref-name>
          <jndi-name>java:app/custom/my-int</jndi-name>
        </resource-ref>
        <resource-ref>
          <res-ref-name>custom/my-string</res-ref-name>
          <jndi-name>java:app/custom/my-string</jndi-name>
        </resource-ref>
    </ejb>
  </enterprise-beans>
</sun-ejb-jar>
