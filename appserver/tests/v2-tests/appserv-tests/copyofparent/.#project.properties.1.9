#### The following properties needs to be changed ####
# Proxy
maven.proxy.host=webcache.sfbay.sun.com
maven.proxy.port=8080

# Where dependencies will be stored.
maven.repo.local=${maven.home.local}/repository

# The OS used
glassfish.os.name=SunOS
glassfish.os.version=9.0
glassfish.cvs.username=anonymous
glassfish.cvsroot=:pserver:${glassfish.cvs.username}@redcvs.red.iplanet.com:/m/jws
glassfish.cvstag=
#glassfish.cvstag=GLASSFISH_BRANCH
# You MUST use absolute path
glassfish.home=/home/${glassfish.cvs.username}/glassfish

# Configuration properties used by the configure-runtime goals.
domain.name=domain1
instance.name=server
admin.usere=admin
admin.password=adminadmin
admin.port=4848
instance.port=8080
orb.port=3700
imq.port=7676
https.port=8181

# List of remote repository
glassfish.repo.hostname=koori.sfbay.sun.com
maven.repo.remote=http://${glassfish.repo.hostname}/java/re/sjsas/nightly/${glassfish.os.name},http://${glassfish.repo.hostname}/java/re/glassfish/nightly/${glassfish.os.name},http://${glassfish.repo.hostname}/java/re/glassfish/external/shared,http://${glassfish.repo.hostname}/java/re/glassfish/external/${glassfish.os.name},http://${glassfish.repo.hostname}/java/re/sjsas/external/shared,http://${glassfish.repo.hostname}/java/re/sjsas/external/${glassfish.os.name},http://www.ibiblio.org/maven

glassfish.module.name=appserv

glassfish.applications.home=${glassfish.home}/lib/install/applications
glassfish.admingui.home=${glassfish.applications.home}/admingui/adminGUI_war
glassfish.databases.home=${glassfish.home}/lib/install/databases
glassfish.dtds.home=${glassfish.home}/lib/dtds
glassfish.image.name=glassfish-image
glassfish.image.version=SNAPSHOT
glassfish.jar.name=${glassfish.image.name}-${glassfish.image.version}
glassfish.jar=${glassfish.jar.name}.jar
glassfish.jsr88.home=${glassfish.home}/lib/deployment
glassfish.lib.home=${glassfish.home}/lib
glassfish.derby.home=${glassfish.home}/derby
glassfish.schemas.home=${glassfish.home}/lib/schemas
glassfish.templates.home=${glassfish.home}/lib/install/templates
glassfish.viewcvs=http://lighthouse.red.iplanet.com/cvsweb/?cvsroot=/m/jws
glassfish.samples.home=${glassfish.home}/samples
glassfish.docs.home=${glassfish.home}/docs

