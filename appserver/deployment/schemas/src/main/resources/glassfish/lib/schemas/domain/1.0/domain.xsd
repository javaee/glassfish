    <xs:complexContent>
      <xs:extension xmlns:dom="http://xmlns.oracle.com/weblogic/domain" base="dom:configurationType">
        <xs:sequence>
          <xs:element name="custom-store" type="xs:string" minOccurs="0" nillable="true">
            <xs:annotation>
              <xs:documentation>The cache store to be used for store backed caches
 (Interface=weblogic.management.configuration.CacheStoreMBean Attribute=getCustomStore)</xs:documentation>
