            <xs:annotation>
              <xs:documentation>(Interface=weblogic.management.configuration.JoltConnectionPoolMBean Attribute=getTrustStorePassPhraseEncryptedAsString)</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
