    <xs:complexContent>
      <xs:extension xmlns:dom="http://xmlns.oracle.com/weblogic/domain" base="dom:configurationType">
        <xs:sequence>
          <xs:element name="realm-class-name" type="xs:string" minOccurs="0" nillable="true">
