      <xs:documentation>Corresponds to WebserviceSecurityConfigurationMBean
 (Interface=weblogic.management.configuration.WebserviceSecurityConfigurationMBean)</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension xmlns:dom="http://xmlns.oracle.com/weblogic/domain" base="dom:configurationType">
        <xs:sequence>
          <xs:element name="class-name" type="xs:string" minOccurs="0" nillable="true">
            <xs:annotation>
