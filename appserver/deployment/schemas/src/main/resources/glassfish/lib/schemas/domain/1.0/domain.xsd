            <xs:simpleType>
              <xs:restriction base="xs:int">
                <xs:minInclusive value="1"/>
                <xs:maxInclusive value="10"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:element>
          <xs:element name="thread-count" minOccurs="0" nillable="false" default="15">
            <xs:annotation>
              <xs:documentation>The number of threads assigned to this queue.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:int">
                <xs:minInclusive value="0"/>
                <xs:maxInclusive value="65536"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:element>
          <xs:element name="queue-length-threshold-percent" minOccurs="0" nillable="false" default="90">
            <xs:annotation>
