      <xs:extension xmlns:dom="http://xmlns.oracle.com/weblogic/domain" base="dom:configurationType">
        <xs:sequence>
          <xs:element name="access-point" type="xs:string" minOccurs="0" nillable="false" default="myRAP">
            <xs:annotation>
              <xs:documentation>The unique name used to identify this remote Tuxedo access
 point. This name should be unique for all local and remote Tuxedo
 access points defined within a WTC Service. This allows you to
 define unique configurations having the same Access Point ID.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="access-point-id" type="xs:string" minOccurs="0" nillable="false" default="myRAPId">
